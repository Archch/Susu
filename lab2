using System;
class HelloWorld {
  static void Main() {
    Double hour, minut, gradus;
    hour = Convert.ToDouble (Console.ReadLine());    Console.WriteLine("hour\n");
    minut = Convert.ToDouble (Console.ReadLine());    Console.WriteLine("minut\n");
    
    gradus = (hour + (minut / 60)) * 30 - minut *6;
    
    Console.WriteLine(Math.Abs(gradus));
  
      
  }
}
